{
    "name": "omni-theme",
    "version": "1.0.0",
    "sencha": {
         "namespace": "Ext",
        "type": "theme",
        "extend": "theme-neptune",
        "toolkit": "classic",
        "creator": "Omni Eshops",
        "summary": "OmniEshops dashboard Base Theme",
        "detailedDescription": "Theme to be used on OmniEshops Dashboard",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "output": "${package.dir}/build",
        "local": true,
        "sass" : {
            "namespace": "Ext",
            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],
            "var": [
                "${package.dir}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src"
            ]
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],

        "example": {
            "path": [
                "${package.dir}/examples"
            ]
        },
        "framework": "ext",
        "requires": []
    }
}
